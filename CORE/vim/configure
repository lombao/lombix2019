unset STRIP
echo '#define SYS_VIMRC_FILE "/usr/etc/vimrc"' >> src/feature.h || exit 1
echo '#define SYS_GVIMRC_FILE "/usr/etc/gvimrc"' >> src/feature.h || exit 1
./configure --prefix=$PREFIX 		\
            --mandir=$PREFIX/share/man 	\
            --with-features=huge 	\
            --with-tlib=ncursesw 

